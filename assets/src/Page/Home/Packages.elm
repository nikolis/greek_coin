type alias Model = 
    {
      session: Session
    , myDrop1State : Dropdown.State
    , feedMeta : Maybe (Dict String AssetMetaInfo)
    , assetPairsCurr : Status (List AssetPairCurrency)
    , selectedPair : Maybe AssetPairCurrency
    }

init: Session -> (Model, Cmd Msg)
init session = 
    let
        md = 
          { session = session 
          , myDrop1State = Dropdown.initialState
          , feedMeta = Nothing
          , assetPairsCurr = Loading
          , selectedPair = Nothing
          }
    in
    (md, Cmd.none)

type alias AssetPairCurrency = 
    {
        alternateName : String
    ,   primaryName : String
    ,   wsName : String
    ,   base : Currency
    ,   quote : Currency
    ,   decimals : Int
    }

type Status a
    = Loading
    | LoadingSlowly
    | Loaded a
    | Failed


type Msg =
    
     MyDrop1Msg Dropdown.State
    | ItemMsg AssetPairCurrency
    | GotAssetPairCurrs (List AssetPairCurrency)
    | ToRegister


update : Msg -> Model -> (Model, Cmd Msg)
update msg model =
    case msg of
         ToRegister ->
             (model, Navigat.load (Route.routeToString Route.Login))

         GotAssetPairCurrs list1 ->
             let
                 theList = List.filter (\pair -> String.contains "EUR" pair.quote.title ) list1
             in
             ({model | assetPairsCurr = Loaded theList, selectedPair = (List.head theList)}, Cmd.none)

         MyDrop1Msg state ->
            ( { model | myDrop1State = state }
            , Cmd.none
            )
         ItemMsg pair ->
             ({ model | selectedPair = Just pair, myDrop1State = Dropdown.initialState}, Cmd.none)


